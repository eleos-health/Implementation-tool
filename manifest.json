{
  "name": "Eleos Health EHR Implementation tools",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "This extension is intended to help us configure new clients on existing EHRs",
  "homepage_url": "http://eleos.health",
  "devtools_page": "src/tools/devtools.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "action": {
    "default_popup": "js/index.html",
    "default_title": "React Chrome Extension"
  },
  "permissions": [
    "alarms",
    "storage",
    "scripting",
    "system.memory"
  ]
}